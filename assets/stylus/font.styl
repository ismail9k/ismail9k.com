@font-face
  font-weight: 500
  font-style: normal
  font-family: 'Fira Code'
  src: local('Fira Code'), url('../fonts/FiraCode-Regular.woff2') format('woff2'), url('../fonts/FiraCode-Regular.woff') format('woff'), url('../fonts/FiraCode-Regular.ttf') format('truetype')
  font-display: swap

@font-face
  font-weight: 700
  font-style: normal
  font-family: 'Fira Code'
  src: local('Fira Code Bold'), url('../fonts/FiraCode-Bold.woff2') format('woff2'), url('../fonts/FiraCode-Bold.woff') format('woff'), url('../fonts/FiraCode-Bold.ttf') format('truetype')
  font-display: swap

html,
body
  font-family: 'Fira Code', -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Helvetica Neue', Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol'
 